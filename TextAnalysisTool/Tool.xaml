<Window x:Class="TextAnalysisTool.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TextAnalysisTool"
        mc:Ignorable="d"
        Title="Keyword Analyzer" Height="350" Width="525">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <TextBox TextChanged="TextBox_TextChanged" Margin="5" Name="Input">
        </TextBox>
        <TextBox IsReadOnly="True" Margin="5" Grid.Column="1" Name="KeyWords">
        </TextBox>
    </Grid>
</Window>
